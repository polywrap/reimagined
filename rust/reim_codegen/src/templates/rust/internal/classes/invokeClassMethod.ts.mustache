pub fn invoke_class_method(buffer: &[u8], external_module: dyn ExternalModule) -> Vec<u8> {
  const class_id = u32::from_be_bytes(buffer.try_into().expect("Class ID must be 4 bytes"));
  const data_buffer = &buffer[4..];

  match class_id {
    {{#types}}
    {{#model.isClass}}
    {{^model.isExternal}}
    WrapManifest.Internal.Class.{{model.name}} =>
        {{model.name}}Wrapped.invoke_method(data_buffer, external_module),
    {{/model.isExternal}}
    {{/model.isClass}}
    {{/types}}
    _ => panic!("Unknown class ID: " + classId.toString()),
  }
}
